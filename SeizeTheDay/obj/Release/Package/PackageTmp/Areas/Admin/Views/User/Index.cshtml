                                                                        <option value=""></option>
                                                                        <option ng-selected="model.UserTypeID==item.userTypeID" ng-repeat="item in lstUserType" value="{{item.userTypeID}}">{{item.userTypeName}}</option>
                                                                    </select>
                                                                    <input type="text" class="form-control" ng-model="model.UserTask" value="{{model.UserTask}}" id="userTask" name="userTask" />
                                                                </div>

                                                                <div class="box box-info">
                                                                    <div class="box-header">
                                                                        <h3 class="box-title" style="color:darkred">Tag UserName and Color Picker</h3>
                                                                    </div>
                                                                    <div class="box-body">
                                                                        <!-- Color Picker -->
