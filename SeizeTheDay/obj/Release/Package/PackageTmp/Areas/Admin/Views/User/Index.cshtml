                                                                        </div>
                                                                        <!-- /.form group -->
                                                                        <!-- Color Picker -->
                                                                        <div class="form-group">
                                                                            <label>Color picker with addon:</label>
                                                                            <div class="input-group my-colorpicker2">
                                                                                <input type="text" ng-model="model.TagColor" value="{{model.TagColor}}" placeholder="Choose a color" class="form-control" />

                                                                                <div class="input-group-addon">
                                                                                    <i></i>
                                                                                </div>
                                                                            </div>
                                                                            <!-- /.input group -->
                                                                        </div>

                                                                    </div>
                                                                    <!-- /.box-body -->
                                                                </div>
                                                                <div class="form-group">
                                                                    <label>Status</label>
                                                                    <select class="form-control" style="width: 100%;" ng-model="model.Status" required>
                                                                        <option value="Active">Active</option>
                                                                        <option value="Unconfirmed">Unconfirmed</option>
                                                                        <option value="Banned">Banned</option>
                                                                    </select>
                                                                </div>
                                                            </div>


                                                            <div class="col-md-6">
                                                                <div class="form-group">
                                                                    <label>Role</label>
                                                                    <ul class="list-unstyled">
                                                                        <li ng-repeat="item in lstRoles">
                                                                            <input type="checkbox" id="{{item.Id}}" value="{{item}}" checklist-model="checkedRole.role" checklist-value="item" />
                                                                            <label for="{{item.Id}}">{{item.Name}}</label>
                                                                        </li>
                                                                    </ul>
                                                                </div>
                                                            </div>

                                                        </div>
                                                    </div>
