
@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>

<script src="~/Areas/Admin/Content/AdminLTE/plugins/colorpicker/bootstrap-colorpicker.min.js"></script>
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper" ng-controller="UserCtrl">
    <!-- Content Header (Page header) -->
    @*<div ng-show="flgMessage" id="message" class="alert alert-dismissible">
